<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AdminIndex</servlet-name>
        <servlet-class>controller.AdminIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>controller.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogout</servlet-name>
        <servlet-class>controller.AdminLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datosFicticios</servlet-name>
        <servlet-class>controller.datosFicticios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>estudiantesPerdiendo</servlet-name>
        <servlet-class>controller.estudiantesPerdiendo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mejoresGrupos</servlet-name>
        <servlet-class>controller.mejoresGrupos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recuperarContrasena</servlet-name>
        <servlet-class>controller.recuperarContrasena</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminBuscarMateria</servlet-name>
        <servlet-class>controller.administrador.AdminBuscarMateria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRegistrarMateria</servlet-name>
        <servlet-class>controller.administrador.AdminRegistrarMateria</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminIndex</servlet-name>
        <url-pattern>/AdminIndex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogout</servlet-name>
        <url-pattern>/AdminLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>datosFicticios</servlet-name>
        <url-pattern>/datosFicticios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>estudiantesPerdiendo</servlet-name>
        <url-pattern>/estudiantesPerdiendo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mejoresGrupos</servlet-name>
        <url-pattern>/mejoresGrupos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recuperarContrasena</servlet-name>
        <url-pattern>/recuperarContrasena</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminBuscarMateria</servlet-name>
        <url-pattern>/AdminBuscarMateria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRegistrarMateria</servlet-name>
        <url-pattern>/AdminRegistrarMateria</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
